<h1>Experiences#index</h1>
<p>Find me in app/views/experiences/index.html.erb</p>
<% @experiences.each do |experience| %>
  <%= experience.name %>
  <%= experience.address %>
  <%= experience.user.github_username %>
  <%= experience.price %>
  <%= link_to 'show', experience_path(experience.id) %>
<% end %>
