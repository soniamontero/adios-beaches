var vote_card = document.querySelector(`[data-votes-id="<%= @vote.experience.id %>"]`)
var vote_show_card = document.querySelector(`[data-show-vote-id="<%= @vote.experience.id %>"]`)

if (vote_card) {
  vote_card.innerHTML = "<%= j render(partial: 'votes-index-and-profile-card', locals: { experience: @vote.experience, user: @user }) %>";
}

if (vote_show_card) {
  vote_show_card.innerHTML = "<%= j render(partial: 'votes-show-card', locals: { experience: @vote.experience }) %>";
}
